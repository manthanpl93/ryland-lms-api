  "engines": {
    "node": ">=20.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run compile && yarn run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "cross-env NODE_ENV=development ts-node-dev --watch --no-notify src/",
    "start": "yarn run compile && node lib/",
    "mocha": "mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc",
    "migrate": "migrate-mongo up",
    "migrate:rollback": "migrate-mongo down",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org naveen07 --project node-express ./lib && sentry-cli sourcemaps upload --org naveen07 --project node-express ./lib"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
